import numpy as np
import matplotlib.pyplot as plt

cl_max = np.linspace(0, 2, 400)

alpha_cl_max = cl_max * 9.64 - 1.6

plt.figure(figsize=(10, 4))
plt.plot(alpha_cl_max, cl_max, '-k', linewidth=1.5)
plt.title("CL_max v. AoA")
plt.xlabel("AoA [degrees]")
plt.ylabel("CL_max")
plt.axis("equal")
plt.grid(True)
plt.tight_layout()
plt.show()